<?php
/**
 * @file
 * Install, update and uninstall functions for the views_iaa_issues module.
 *
 */

/**
 * Implements hook_install()
 */
function views_iaa_issues_install() {
  db_update('system')
  ->fields(array(
    'weight' => '50',
  ))
  ->condition('name', 'views_iaa_issues')
  ->condition('type', 'module')
  ->execute();
}
